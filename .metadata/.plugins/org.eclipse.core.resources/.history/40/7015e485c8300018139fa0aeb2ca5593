package com.bridgeit.datastructures;

import javax.xml.soap.Node;

public class QueueAnagramImpl<T>
{
int count=0;
Node front=null,temp;


public QueueAnagramImpl()
{
	
}

public  QueueAnagramImpl(int num)
{
	int[] array=new int[num];
	for(int i-0;i<num;i++)
	{
		enqueue(i);
		
	}
}

public class Node<T>
{
	T data;
	Node next;
	
	public T  getData()
	{
		return data;
		
	}
	
	public void setData(T data)
	{
		this.data=data;
	}
	
	
	public Node getNext()
	{
		return next;
		
	}
	
	public void setNext(Node next)
	{
		this.next= getNext();
		
	}
	

	public Node(T data)
	{
		this.data=data;
		next=null;
		
	}
	
	
}

public<T> void enqueue(T element)
{
	Node n=new Node<T>(element);
	if(front==null)
	{
		front=n;
		temp=n;
		count++;
		
	}
	else
	{
		temp.next=n;
		temp=temp.next;
		count++;
	}
}

public void dequeue()
{
	if(front==null)
	{
		System.out.println("list is empty");
	}
	else if(front!=null)
	{
		front=front.next;
		count--;
	}
}

public boolean isEmpty()
{
	if(count==0)
		return true;
	else
		return false;
}

public int size()
{
	return count;
}
public void getelement()
{
	temp=front;
	for(int i=0;i<size();i++)
	{
		System.out.println(temp.getData());
		temp=temp.getNext();
	}
}



}
