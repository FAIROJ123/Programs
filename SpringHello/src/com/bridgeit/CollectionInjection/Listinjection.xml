<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               


<bean id="noString" class="com.bridgeit.CollectionInjection.NoStringExam">
<constructor-arg  value="Phani"></constructor-arg>
<constructor-arg  value="111"></constructor-arg>
<constructor-arg  value="AngularJs"></constructor-arg>
</bean> 

<bean id="noString1" class="com.bridgeit.CollectionInjection.NoStringExam">
<constructor-arg index="1" value="222"></constructor-arg>
<constructor-arg index="0" value="kiran"></constructor-arg>
<constructor-arg index="2" value="AngularJs"></constructor-arg>
</bean> 


<bean id="list" class="com.bridgeit.CollectionInjection.Person">
<constructor-arg  value="Fairoj"></constructor-arg>
<constructor-arg value="123"></constructor-arg>
<constructor-arg>
<list>
<value>Java File</value>
<value>JavaScript File</value>
<value>AngularJs File</value>
</list>
</constructor-arg>

<constructor-arg>
<list>
<ref bean="noString"/>
<ref bean="noString1"/>
</list>
</constructor-arg>

</bean>
</beans>